{
  "name": "@stoe/template",
  "version": "0.0.0-development",
  "description": "NodeJS Template",
  "keywords": [
    "node",
    "template"
  ],
  "author": {
    "name": "Stefan StÃ¶lzle",
    "email": "stefan@stoelzle.me",
    "url": "https://github.com/stoe"
  },
  "repository": "https://github.com/stoe/node-template.git",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "main": "index.js",
  "scripts": {
    "format": "npx prettier --config-precedence prefer-file --write . && eslint . --fix",
    "prepare": "husky",
    "pretest": "npx eslint-config-prettier eslint.config.js",
    "test": "eslint ."
  },
  "devDependencies": {
    "@github/prettier-config": "^0.0.6",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": ".husky/pre-commit",
      "pre-push": ".husky/pre-push"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run test"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prettier": "@github/prettier-config"
}
